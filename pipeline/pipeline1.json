								}
							},
							{
								"source": {
									"name": "first_name",
									"type": "String"
								},
								"sink": {
									"name": "name",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "phone_number",
									"type": "String"
								},
								"sink": {
									"name": "phon"
								}
							}
						]
